<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.10.xsd">

  <changeSet author="weigel" id="add_fetch_since_months_calendar">
    <preConditions>
      <tableExists tableName="Calendar"/>
      <not>
        <columnExists tableName="Calendar" columnName="fetchSinceMonths"/>
      </not>
    </preConditions>

    <addColumn tableName="Calendar">
      <column name="fetchSinceMonths" type="INT(2)" defaultValue="12">
        <constraints nullable="false"/>
      </column>
    </addColumn>

  </changeSet>
</databaseChangeLog>
